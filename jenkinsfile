pipeline{
  agent any

  stages{
    stage('Check out'){
      steps{
        checkout scm
      }
    }

    stage('npm install'){
      steps{
        sh 'npm install'
      }
    }

    stage('build'){
      steps{
        sh 'npm run build'
      }
    }

    // stage('test'){
    //   steps{
    //     sh 'npm run test'
    //   }
    // }
  }
}
